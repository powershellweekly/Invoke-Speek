  <Code><![CDATA[<#
Program Name: Invoke-Speak
Author: Michael J. Thomas
Company: Thomas IT Services
Contact: Mike@ThomasITServices.com
Created:  06/20/2019
Modified: 06/21/2019
Notes: Invoke-Speak is used to speak to the local or remote computer.
• Program starts with loaded installed voices from local computer.
• Right Click Voice ComboBox to Load Local or Remote Voices.
• Adjust the Voice Rate with the Speed Slider.
• Click the Speak Button to Speak Written Text in The Input Box.
• Enter a Computer Name to Speak to a Remote Computer.
• Click the Stop Button to Stop Speaking on the Local Computer.
• The Statusbar notifies you a command is running and if you forgot something.
• Right Click Speak Button for Clear Options for Input and Computer Name Text Boxes.

#>



$frmMain_Load={
	#Initialize Form Controls here
	
	#Update ComboBox with the available Voices Installed on the System.
	Update-ComboBox $cboVoice $Speak.GetInstalledVoices().VoiceInfo.Name
	$cboVoice.Text = $Speak.Voice.Name
	$tbarSpeed.Value = $Speak.Rate
	
	
}

#region Control Helper Functions
function Update-ComboBox
{
<#
	.SYNOPSIS
		This functions helps you load items into a ComboBox.
	
	.DESCRIPTION
		Use this function to dynamically load items into the ComboBox control.
	
	.PARAMETER ComboBox
		The ComboBox control you want to add items to.
	
	.PARAMETER Items
		The object or objects you wish to load into the ComboBox's Items collection.
	
	.PARAMETER DisplayMember
		Indicates the property to display for the items in this control.
		
	.PARAMETER ValueMember
		Indicates the property to use for the value of the control.
	
	.PARAMETER Append
		Adds the item(s) to the ComboBox without clearing the Items collection.
	
	.EXAMPLE
		Update-ComboBox $combobox1 "Red", "White", "Blue"
	
	.EXAMPLE
		Update-ComboBox $combobox1 "Red" -Append
		Update-ComboBox $combobox1 "White" -Append
		Update-ComboBox $combobox1 "Blue" -Append
	
	.EXAMPLE
		Update-ComboBox $combobox1 (Get-Process) "ProcessName"
	
	.NOTES
		Additional information about the function.
#>
	
	param
	(
		[Parameter(Mandatory = $true)]
		[ValidateNotNull()]
		[System.Windows.Forms.ComboBox]
		$ComboBox,
		[Parameter(Mandatory = $true)]
		[ValidateNotNull()]
		$Items,
		[Parameter(Mandatory = $false)]
		[string]$DisplayMember,
		[Parameter(Mandatory = $false)]
		[string]$ValueMember,
		[switch]
		$Append
	)
	
	if (-not $Append)
	{
		$ComboBox.Items.Clear()
	}
	
	if ($Items -is [Object[]])
	{
		$ComboBox.Items.AddRange($Items)
	}
	elseif ($Items -is [System.Collections.IEnumerable])
	{
		$ComboBox.BeginUpdate()
		foreach ($obj in $Items)
		{
			$ComboBox.Items.Add($obj)
		}
		$ComboBox.EndUpdate()
	}
	else
	{
		$ComboBox.Items.Add($Items)
	}
	
	$ComboBox.DisplayMember = $DisplayMember
	$ComboBox.ValueMember = $ValueMember
}
#endregion

$btnSpeak_Click = {
	#Invoke-Speak -ComputerName $ComputerName
	$ComputerName = $txtComputerName.Text.Trim()
	$Input = $txtInput.Text
	
	If ([string]::IsNullOrWhiteSpace($Input))
	{
		$txtStatusBar.Text = "I shall not speak words that are not written!"
		Start-sleep 3
		$txtStatusBar.Text = "Please write some words and then I shall speak!"
		Start-Sleep 3
		$txtStatusBar.Text = ""
			
	}
	Else
	{
		
		If ([string]::IsNullOrWhiteSpace($ComputerName))
		{
			$Speak.Rate = $tbarSpeed.Value
			$Speak.SelectVoice($cboVoice.Text)
			$Speak.SpeakAsync(($txtInput.Text))
			$txtStatusBar.Text = "Speaking on Local Computer"
			Start-Sleep 3
			$txtStatusBar.Text = ""
		}
		Else
		{
			$Voice = $cboVoice.Text
			$Speed = $tbarSpeed.Value
			$Words = $txtInput.Text
			$txtStatusBar.Text = "Speaking on Remote Computer"
			Start-Sleep 3
			$txtStatusBar.Text = ""
			
			Invoke-Command -ComputerName $ComputerName -ScriptBlock {
				Add-Type -AssemblyName System.Speech
				$Speak = New-Object System.Speech.Synthesis.SpeechSynthesizer
				$Speak.Rate = $Using:Speed
				$Speak.SelectVoice($Using:Voice)
				$Speak.Speak(($Using:Words))
				$Speak.Dispose()
			} -AsJob
		}
	}
}

