$loadRemoteVoicesToolStripMenuItem_Click={
	
	
	#Load Remote Computer Voices
	$ComputerName = $txtComputerName.Text.Trim()
	If ([string]::IsNullOrWhiteSpace($ComputerName))
	{
		#Do Nothing
		$txtStatusBar.Text = "Please Enter Remote Computer Name to Load Remote Voices"
		Start-Sleep 3
		$txtStatusBar.Text = ""
	}
	Else
	{
		#Clear Current Voices in ComboBox
		$cboVoice.Text = ""
		$cboVoice.Items.Clear()
		
		#Get Remote Computer Installed Voices
		$RemoteVoices = Invoke-Command -ComputerName $ComputerName -ScriptBlock {
			Add-Type -AssemblyName System.Speech
			$Speak = New-Object System.Speech.Synthesis.SpeechSynthesizer
			$Speak.GetInstalledVoices().VoiceInfo.Name
		}
		
		foreach ($RemoteVoice in $RemoteVoices)
		{
			$cboVoice.Items.Add($RemoteVoice)
			
		}
		$cboVoice.Text = $Speak.Voice.Name
	}
	
	
}

